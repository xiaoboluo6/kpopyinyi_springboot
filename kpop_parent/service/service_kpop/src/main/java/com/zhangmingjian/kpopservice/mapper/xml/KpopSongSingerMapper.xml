<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhangmingjian.kpopservice.mapper.KpopSongSingerMapper">
    <delete id="hardDeleteBySongId" parameterType="String">
        DELETE FROM kpop_song_singer
        WHERE song_id = #{songId}
    </delete>
</mapper>
